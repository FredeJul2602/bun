{
  "name": "claude-skills-assistant",
  "version": "1.0.0",
  "description": "Web application for interacting with Claude Skills via MCP",
  "module": "src/server/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run build && bun run --watch src/server/index.ts",
    "dev:client": "bun build src/client/index.tsx --outdir dist/client --watch",
    "build": "bun build src/client/index.tsx --outdir dist/client --minify && cp public/index.html dist/client/index.html",
    "start": "bun run src/server/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/static": "^1.4.7",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "elysia": "^1.4.22",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.8.0"
  }
}
